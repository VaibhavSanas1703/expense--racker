<section id="sign-up-form">
       <div id="main-form-container">
        <div>
              <h1 id="sign-up-heading">Sign up</h1>
       </div>
       <hr style="margin: 10px 0; border: 0; width: 100%; height: 0.5px; background: rgba(255,255,255,0.1);">
       <div>
              <form action="/user/auth/sign-up" method="post" id="sign-up-form-main">
                <%- include("../partials/errors") %>
                     <div>
                         <label for="username">Username <i class="fi fi-rr-user"></i></label>
                         <input type="text" name="username">
                     </div>
                     <div>
                         <label for="email">Email <i class="fi fi-rr-envelope"></i></label>
                         <input type="email" name="email">
                     </div>
                     <div>
                         <label for="password">Create Password <i class="fi fi-rr-lock"></i></label>
                         <input type="password" name="password">
                     </div>
                     <hr style="margin: 10px 0; border: 0; width: 100%; height: 0.5px; background: rgba(255,255,255,0.1);">
                     <div id="sign-up-google">
                         <a href="/auth/google">Sign up with <img src="https://www.gstatic.com/marketing-cms/assets/images/d5/dc/cfe9ce8b4425b410b49b7f2dd3f3/g.webp=s96-fcrop64=1,00000000ffffffff-rw" alt=""></a>
                     </div>
                     <div id="already-have">
                         <p>Already Have an Account? <a href="/user/auth/sign-in">Sign in</a></p>
                     </div>
                     <div>
                         <button type="submit">Sign up</button>
                     </div>
              </form>
       </div>
       </div>
</section>
